{
  "name": "flowchart-g",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <22.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --config=jest.config.cjs",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "test:coverage": "jest --config=jest.config.cjs --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "cypress:headless:cjs": "cypress run --config-file cypress.config.cjs",
    "analyze": "vite build --mode analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "docker:build": "docker build -t flowchart-g .",
    "docker:run": "docker run -p 8080:80 flowchart-g",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "start": "npm run preview",
    "predeploy": "npm run build",
    "deploy:github": "npm install -D gh-pages && npx gh-pages -d dist"
  },
  "dependencies": {
    "dompurify": "^3.0.8",
    "fabric": "^5.3.0",
    "lucide-react": "^0.344.0",
    "nanoid": "^5.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/dompurify": "^3.0.5",
    "@types/fabric": "^5.3.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "cypress": "^13.6.4",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
